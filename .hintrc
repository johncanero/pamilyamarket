    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not opera <= 99",
    "not samsung <= 21",
    "not and_chr <= 125",
    "not chrome <= 125",
    "not edge <= 125",
    "not opera <= 110",
    "not samsung <= 25",
    "not and_ff <= 126",
    "not firefox <= 126",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ]
}